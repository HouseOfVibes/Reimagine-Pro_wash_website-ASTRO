    <style>
        :root {
            /* Primary Brand Colors */
            --primary-blue: #00a1f0;
            --navy-blue: #092441;
            --deep-blue: #003463;
            --medium-blue: #0862b7;
            --light-blue: #6ccdf7;
            
            /* Neutrals */
            --silver: #95a3ae;
            --charcoal: #04070c;
            --mid-gray: #737a7e;
            --teal-gray: #8b99a0;
            --off-white: #fefefb;
            --pure-white: #ffffff;
            
            /* Brand Gradients */
            --gradient-primary: linear-gradient(135deg, #092441 0%, #003463 50%, #00a1f0 100%);
            --gradient-cta: linear-gradient(135deg, #00a1f0 0%, #0862b7 100%);
            --gradient-cta-hover: linear-gradient(135deg, #6ccdf7 0%, #00a1f0 100%);
            --gradient-overlay-dark: linear-gradient(135deg, rgba(4,7,12,0.8) 0%, rgba(9,36,65,0.6) 100%);
        }

        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }

        body {
            font-family: 'Open Sans', sans-serif;
            line-height: 1.6;
            color: var(--charcoal);
            padding-top: 75px;
        }

        /* Header Styles */
        .rpw-main-header {
            background: var(--pure-white);
            box-shadow: 0 2px 20px rgba(9, 36, 65, 0.1);
            position: fixed;
            top: 0;
            left: 0;
            right: 0;
            z-index: 1000;
            transition: all 0.3s ease;
        }

        .rpw-main-header.scrolled {
            box-shadow: 0 2px 30px rgba(9, 36, 65, 0.15);
            background: rgba(255, 255, 255, 0.95);
            backdrop-filter: blur(10px);
        }

        .rpw-top-bar {
            background: var(--gradient-primary);
            color: var(--off-white);
            padding: 8px 0;
            font-size: 0.85rem;
        }

        .rpw-top-bar-content {
            max-width: 1200px;
            margin: 0 auto;
            padding: 0 20px;
            display: flex;
            justify-content: space-between;
            align-items: center;
        }

        .rpw-top-bar-left {
            display: flex;
            align-items: center;
            gap: 25px;
        }

        .rpw-top-bar-item {
            display: flex;
            align-items: center;
            gap: 8px;
            font-weight: 500;
        }

        .rpw-top-bar-item i {
            color: var(--light-blue);
        }

        .rpw-top-bar-right {
            display: flex;
            align-items: center;
            gap: 15px;
        }

        .rpw-social-link-top {
            color: var(--off-white);
            font-size: 1rem;
            transition: all 0.3s ease;
            text-decoration: none;
        }

        .rpw-social-link-top:hover {
            color: var(--light-blue);
            transform: translateY(-1px);
        }

        .rpw-main-nav {
            padding: 10px 0;
        }

        .rpw-nav-content {
            max-width: 1200px;
            margin: 0 auto;
            padding: 0 20px;
            display: flex;
            justify-content: space-between;
            align-items: center;
        }

        .rpw-logo-container {
            display: flex;
            align-items: center;
        }

        .rpw-brand-text {
            font-family: 'Poppins', sans-serif;
            font-size: 1.5rem;
            font-weight: 800;
            color: var(--navy-blue);
            text-decoration: none;
            letter-spacing: -0.02em;
            transition: color 0.3s ease;
            background: var(--gradient-primary);
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
            background-clip: text;
        }

        .rpw-brand-text:hover {
            opacity: 0.8;
        }

        .rpw-brand-tagline {
            font-size: 0.65rem;
            font-weight: 400;
            color: var(--primary-blue);
            margin-top: -5px;
            letter-spacing: 0.1em;
            text-transform: uppercase;
        }

        .rpw-nav-menu {
            display: flex;
            list-style: none;
            margin: 0;
            padding: 0;
            align-items: center;
            gap: 35px;
        }

        .rpw-nav-item {
            position: relative;
        }

        .rpw-nav-link {
            font-family: 'Poppins', sans-serif;
            font-weight: 500;
            font-size: 1rem;
            color: var(--navy-blue);
            text-decoration: none;
            padding: 10px 0;
            position: relative;
            transition: all 0.3s ease;
        }

        .rpw-nav-link::after {
            content: '';
            position: absolute;
            bottom: -5px;
            left: 0;
            width: 0;
            height: 2px;
            background: linear-gradient(135deg, #0862b7 0%, #00a1f0 50%, #6ccdf7 100%);
            transition: width 0.3s ease;
        }

        .rpw-nav-link:hover {
            color: var(--primary-blue);
        }

        .rpw-nav-link:hover::after,
        .rpw-nav-link.active::after {
            width: 100%;
        }

        /* Dropdown Menu */
        .rpw-nav-dropdown {
            position: relative;
        }

        .rpw-dropdown-content {
            display: none;
            position: absolute;
            top: 100%;
            left: 0;
            background: var(--pure-white);
            min-width: 240px;
            box-shadow: 0 8px 25px rgba(9, 36, 65, 0.15);
            border-radius: 8px;
            padding: 12px 0;
            margin-top: 10px;
            z-index: 1000;
        }

        .rpw-nav-dropdown:hover .rpw-dropdown-content {
            display: block;
        }

        .rpw-dropdown-content a {
            display: block;
            padding: 12px 20px;
            text-decoration: none;
            color: var(--navy-blue);
            font-weight: 500;
            font-size: 0.95rem;
            transition: all 0.3s ease;
        }

        .rpw-dropdown-content a:hover {
            background: var(--primary-blue);
            color: var(--pure-white);
        }

        .rpw-header-cta {
            display: flex;
            align-items: center;
            gap: 15px;
        }

        .rpw-phone-header {
            display: flex;
            align-items: center;
            gap: 8px;
            color: var(--navy-blue);
            text-decoration: none;
            font-weight: 600;
            transition: color 0.3s ease;
        }

        .rpw-phone-header:hover {
            color: var(--primary-blue);
        }

        .rpw-phone-header i {
            color: var(--primary-blue);
            font-size: 1.1rem;
        }

        .rpw-cta-button {
            background: linear-gradient(135deg, #0862b7 0%, #00a1f0 50%, #6ccdf7 100%);
            color: var(--pure-white);
            padding: 12px 25px;
            border-radius: 25px;
            font-family: 'Poppins', sans-serif;
            font-weight: 600;
            font-size: 0.95rem;
            text-decoration: none;
            transition: all 0.3s ease;
            box-shadow: 0 4px 15px rgba(0, 161, 240, 0.3);
            border: none;
            cursor: pointer;
        }

        .rpw-cta-button:hover {
            transform: translateY(-2px);
            box-shadow: 0 6px 20px rgba(0, 161, 240, 0.4);
        }

        .rpw-mobile-menu-toggle {
            display: none;
            flex-direction: column;
            cursor: pointer;
            padding: 5px;
            background: none;
            border: none;
        }

        .rpw-hamburger {
            width: 25px;
            height: 3px;
            background: var(--navy-blue);
            margin: 3px 0;
            transition: 0.3s;
            border-radius: 2px;
        }

        .rpw-mobile-menu-toggle.active .rpw-hamburger:nth-child(1) {
            transform: rotate(-45deg) translate(-5px, 6px);
        }

        .rpw-mobile-menu-toggle.active .rpw-hamburger:nth-child(2) {
            opacity: 0;
        }

        .rpw-mobile-menu-toggle.active .rpw-hamburger:nth-child(3) {
            transform: rotate(45deg) translate(-5px, -6px);
        }

        .rpw-mobile-menu {
            position: absolute;
            top: 100%;
            left: 0;
            right: 0;
            background: var(--pure-white);
            box-shadow: 0 5px 20px rgba(9, 36, 65, 0.1);
            opacity: 0;
            visibility: hidden;
            transform: translateY(-10px);
            transition: all 0.3s ease;
        }

        .rpw-mobile-menu.active {
            opacity: 1;
            visibility: visible;
            transform: translateY(0);
        }

        .rpw-mobile-menu-content {
            padding: 20px;
        }

        .rpw-mobile-nav-menu {
            list-style: none;
            padding: 0;
            margin: 0 0 20px 0;
        }

        .rpw-mobile-nav-item {
            margin-bottom: 15px;
        }

        .rpw-mobile-nav-link {
            font-family: 'Poppins', sans-serif;
            font-weight: 500;
            font-size: 1.1rem;
            color: var(--navy-blue);
            text-decoration: none;
            display: block;
            padding: 10px 0;
            border-bottom: 1px solid rgba(149, 163, 174, 0.1);
            transition: color 0.3s ease;
        }

        .rpw-mobile-nav-link:hover {
            color: var(--primary-blue);
        }

        /* Mobile Dropdown */
        .rpw-mobile-dropdown {
            position: relative;
        }

        .rpw-mobile-dropdown-content {
            display: none;
            background: rgba(0, 161, 240, 0.05);
            padding-left: 20px;
            margin-top: 5px;
        }

        .rpw-mobile-dropdown.active .rpw-mobile-dropdown-content {
            display: block;
        }

        .rpw-mobile-nav-sublink {
            display: block;
            padding: 10px 0;
            text-decoration: none;
            color: var(--navy-blue);
            font-size: 0.95rem;
            font-weight: 500;
            border-bottom: 1px solid rgba(0, 161, 240, 0.1);
            transition: color 0.3s ease;
        }

        .rpw-mobile-nav-sublink:hover {
            color: var(--primary-blue);
        }

        .rpw-mobile-cta {
            display: flex;
            flex-direction: column;
            gap: 15px;
            align-items: center;
        }

        .rpw-mobile-phone {
            display: flex;
            align-items: center;
            gap: 8px;
            color: var(--navy-blue);
            text-decoration: none;
            font-weight: 600;
            font-size: 1.1rem;
        }

        .rpw-mobile-phone i {
            color: var(--primary-blue);
        }

        @media (max-width: 768px) {
            .rpw-top-bar {
                display: none;
            }

            body {
                padding-top: 60px;
            }

            .rpw-main-nav {
                padding: 8px 0;
                position: relative;
            }

            .rpw-brand-text {
                font-size: 1.2rem;
            }

            .rpw-brand-tagline {
                font-size: 0.55rem;
            }

            .rpw-nav-menu,
            .rpw-header-cta .rpw-phone-header {
                display: none;
            }

            .rpw-mobile-menu-toggle {
                display: flex;
            }

            .rpw-header-cta {
                gap: 10px;
            }

            .rpw-cta-button {
                padding: 10px 20px;
                font-size: 0.9rem;
            }

            .hero-logo img {
                max-width: 400px;
            }
        }

        @media (max-width: 480px) {
            .rpw-nav-content {
                padding: 0 15px;
            }

            .rpw-brand-text {
                font-size: 1rem;
            }

            .rpw-brand-tagline {
                font-size: 0.5rem;
            }

            .rpw-cta-button {
                padding: 8px 16px;
                font-size: 0.85rem;
            }

            .hero-logo {
                padding-top: 2rem;
            }

            .hero-logo img {
                max-width: 300px;
            }

            .hero-content {
                padding: 1.5rem 1rem;
            }

            .hero-description {
                font-size: 1rem;
                margin-bottom: 2rem;
            }

            .btn-primary, .btn-secondary {
                padding: 1rem 2rem;
                font-size: 1rem;
            }

            .results-showcase-section,
            .trust-section,
            .services-section,
            .service-area-section,
            .contact-section {
                padding: 60px 15px;
            }

            .results-showcase-container h2,
            .trust-section h2,
            .services-section h2,
            .service-area-section h2,
            .contact-section h2 {
                font-size: 2rem;
            }

            .comparison-image-wrapper {
                height: 300px;
            }

            .comparison-labels span {
                font-size: 1rem;
                padding: 8px 20px;
            }
        }

        @media (max-width: 360px) {
            .hero-logo img {
                max-width: 250px;
            }

            .hero-title {
                font-size: 1.75rem;
            }

            .hero-subtitle {
                font-size: 1.25rem;
            }
        }

        .hero-section {
            min-height: 100vh;
            display: flex;
            align-items: center;
            justify-content: center;
            position: relative;
            overflow: hidden;
        }

        /* Animated background image layer */
        .hero-section::after {
            content: '';
            position: absolute;
            top: -10%;
            left: -10%;
            right: -10%;
            bottom: -10%;
            background-size: cover;
            background-position: center;
            background-repeat: no-repeat;
            background-image: var(
                --hero-background-fallback,
                url('/images/hero-background.png')
            );
            background-image: var(
                --hero-background-image,
                var(
                    --hero-background-fallback,
                    url('/images/hero-background.png')
                )
            );
            z-index: 0;
            animation: waterMove 40s ease-in-out infinite;
        }

        @keyframes waterMove {
            0%, 100% {
                transform: translate(0, 0) scale(1.1);
            }
            25% {
                transform: translate(-2%, 1%) scale(1.12) rotate(0.5deg);
            }
            50% {
                transform: translate(2%, -1%) scale(1.1) rotate(-0.5deg);
            }
            75% {
                transform: translate(-1%, 2%) scale(1.11) rotate(0.3deg);
            }
        }

        /* Dark gradient overlay with animated water effects */
        .hero-section::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            background:
                var(--gradient-overlay-dark),
                radial-gradient(ellipse at 20% 80%, rgba(108, 221, 247, 0.15) 0%, transparent 50%),
                radial-gradient(ellipse at 80% 20%, rgba(0, 161, 240, 0.15) 0%, transparent 50%),
                radial-gradient(ellipse at 40% 40%, rgba(8, 98, 183, 0.1) 0%, transparent 50%),
                radial-gradient(circle at 15% 30%, rgba(108, 221, 247, 0.2) 0%, transparent 15%),
                radial-gradient(circle at 85% 70%, rgba(0, 161, 240, 0.2) 0%, transparent 15%),
                radial-gradient(circle at 50% 50%, rgba(108, 221, 247, 0.15) 0%, transparent 20%),
                radial-gradient(circle at 30% 90%, rgba(0, 161, 240, 0.2) 0%, transparent 15%),
                radial-gradient(circle at 70% 10%, rgba(108, 221, 247, 0.15) 0%, transparent 15%);
            z-index: 1;
            animation: waterFlow 8s linear infinite;
            pointer-events: none;
        }

        @keyframes waterFlow {
            0% {
                opacity: 1;
                filter: brightness(1) hue-rotate(0deg);
            }
            25% {
                opacity: 0.95;
                filter: brightness(1.05) hue-rotate(2deg);
            }
            50% {
                opacity: 1;
                filter: brightness(1) hue-rotate(0deg);
            }
            75% {
                opacity: 0.95;
                filter: brightness(1.05) hue-rotate(-2deg);
            }
            100% {
                opacity: 1;
                filter: brightness(1) hue-rotate(0deg);
            }
        }

        .hero-content {
            text-align: center;
            color: var(--off-white);
            max-width: 900px;
            padding: 2rem;
            position: relative;
            z-index: 2;
        }

        .hero-logo {
            margin-bottom: 2rem;
            padding-top: 3rem;
        }

        .hero-logo img {
            max-width: 500px;
            width: 100%;
            height: auto;
            mix-blend-mode: screen;
            filter: drop-shadow(0 0 30px rgba(108, 221, 247, 0.5)) drop-shadow(0 0 60px rgba(0, 161, 240, 0.3));
            animation: logoFloat 6s ease-in-out infinite;
        }

        @keyframes logoFloat {
            0%, 100% {
                transform: translateY(0px) scale(1);
            }
            50% {
                transform: translateY(-15px) scale(1.05);
            }
        }

        .hero-title {
            font-family: 'Poppins', sans-serif;
            font-size: clamp(2rem, 5vw, 3.5rem);
            font-weight: 800;
            margin-bottom: 0.5rem;
            text-shadow: 2px 2px 8px rgba(0,0,0,0.3);
            letter-spacing: -0.02em;
            line-height: 1.1;
            text-align: center;
            display: block;
            width: 100%;
            margin-left: auto;
            margin-right: auto;
        }

        .hero-subtitle {
            font-family: 'Poppins', sans-serif;
            font-size: clamp(1.5rem, 3vw, 2rem);
            font-weight: 300;
            color: var(--light-blue);
            margin-bottom: 1.5rem;
            text-shadow: 1px 1px 4px rgba(0,0,0,0.3);
        }

        .hero-description {
            font-size: clamp(1.1rem, 2vw, 1.3rem);
            margin-bottom: 3rem;
            max-width: 700px;
            margin-left: auto;
            margin-right: auto;
            opacity: 0.95;
            line-height: 1.6;
        }

        .hero-buttons {
            display: flex;
            gap: 1rem;
            justify-content: center;
            flex-wrap: wrap;
            margin-bottom: 3rem;
        }

        .btn-primary {
            background: var(--gradient-cta);
            color: var(--pure-white);
            padding: 1.2rem 2.5rem;
            border-radius: 50px;
            font-weight: 700;
            font-size: 1.1rem;
            text-decoration: none;
            display: inline-block;
