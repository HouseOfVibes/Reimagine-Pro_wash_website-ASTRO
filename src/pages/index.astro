import FAQSchema from '../components/FAQSchema.astro';
import logo from '../../public/images/logo.png';
import heroBackground from '../../public/images/hero-background.png';
import insuredBadge from '../../public/images/insured-badge.png';
import ecoFriendlyBadge from '../../public/images/eco-friendly-badge.png';
import multiPanelCollage from '../../public/images/before-after-collage.png';
import poolDeck from '../../public/images/pool-deck-cleaning.png';
import houseWashing from '../../public/images/house-washing.png';
import hardscapes from '../../public/images/hardscapes-cleaning.png';
import gutterCleaning from '../../public/images/gutter-cleaning.png';
import commercial from '../../public/images/commercial-pressure-washing.png';
import deckRestoration from '../../public/images/deck-restoration.png';
import fenceCleaning from '../../public/images/clean-fence.png';
import brickRestoration from '../../public/images/brick-restoration.png';
import graffitiRemoval from '../../public/images/graffiti-removal-before-after.png';

const heroBackgroundWebp = await getImage({
    src: heroBackground,
    format: 'webp',
    width: 1920,
    quality: 72,
});

const heroBackgroundPng = await getImage({
    src: heroBackground,
    format: 'png',
    width: 1920,
});

const heroBackgroundImageSet = `image-set(url('${heroBackgroundWebp.src}') type('image/webp') 1x, url('${heroBackgroundPng.src}') type('image/png') 1x)`;
const heroBackgroundFallbackValue = `url('${heroBackgroundPng.src}')`;

const faqs = [
  {
    question: "What's the difference between pressure washing and soft washing?",
    answer: "Pressure washing uses high-pressure water to remove dirt and grime from hard surfaces like driveways and concrete. Soft washing uses low-pressure water combined with specialized cleaning solutions, making it perfect for delicate surfaces like siding, roofs, and decks. We use the appropriate method for each surface to ensure safe and effective cleaning."
  },
  {
    question: "How often should I have my house pressure washed?",
    answer: "Most homes benefit from professional pressure washing once a year. However, homes in shaded areas or near trees may need cleaning twice a year due to increased mold and mildew growth. Regular cleaning not only maintains your home's appearance but also prevents long-term damage from organic growth."
  },
  {
    question: "Is pressure washing safe for all surfaces?",
    answer: "When done correctly by professionals, pressure washing is safe for most exterior surfaces. We adjust our pressure settings and cleaning methods based on the materialâ€”using soft washing for delicate surfaces like vinyl siding, wood, and shingles, and higher pressure for concrete and brick. Our team is trained to identify the right approach for each surface."
  },
  {
    question: "Will pressure washing damage my landscaping?",
    answer: "No. We take precautions to protect your landscaping, including pre-wetting plants, using eco-friendly cleaning solutions that are safe for vegetation, and rinsing thoroughly after cleaning. Our biodegradable products are designed to be gentle on plants while tough on dirt and grime."
  },
  {
    question: "How long does a typical pressure washing job take?",
    answer: "The time varies depending on the size and scope of the project. A standard house washing typically takes 2-4 hours, while driveways and patios can take 1-3 hours. Larger commercial projects may take longer. We'll provide an estimated timeframe when we give you your free quote."
  }
];
